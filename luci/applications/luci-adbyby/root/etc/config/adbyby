config adbyby 'config'    
        option enabled '0'
        option forward '1'
        option listen_address '192.168.1.1'
        option listen_port '8118'
        option debug '0'
        option enable_remote_http_toggle '0'
        option buffer_limit '4096'
        option keep_alive_timeout '30'
        option socket_timeout '300'
        option log_messages '1'
	option rule 'video,lazy'

